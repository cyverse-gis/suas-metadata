<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<?import java.net.URL?>
<BorderPane fx:controller="controller.homeView.CreditsController" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml">
    <padding>
        <Insets top="10" bottom="10" right="10" left="10"/>
    </padding>
    <stylesheets>
        <URL value="@Credits.css"/>
    </stylesheets>
    <top>
        <Label BorderPane.alignment="CENTER" text="Calliope Credits">
            <font>
                <Font size="32"/>
            </font>
            <padding>
                <Insets top="5" bottom="5"/>
            </padding>
        </Label>
    </top>
    <center>
        <GridPane styleClass="creditsPane" BorderPane.alignment="CENTER" alignment="CENTER" vgap="10" hgap="20">
            <columnConstraints>
                <ColumnConstraints/>
                <ColumnConstraints/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints/>
                <RowConstraints/>
                <RowConstraints/>
            </rowConstraints>
            <Label GridPane.columnIndex="0" GridPane.rowIndex="0" text="Project Lead:"/>
            <Label GridPane.columnIndex="1" GridPane.rowIndex="0" text="Tyson Swetnam"/>
            <Label GridPane.columnIndex="0" GridPane.rowIndex="1" text="Calliope Developer:"/>
            <Label GridPane.columnIndex="1" GridPane.rowIndex="1" text="David Slovikosky"/>
            <Label GridPane.columnIndex="0" GridPane.rowIndex="2" text="Backend Research and Developer:"/>
            <Label GridPane.columnIndex="1" GridPane.rowIndex="2" text="Pooja Narayan"/>
        </GridPane>
    </center>
</BorderPane>
