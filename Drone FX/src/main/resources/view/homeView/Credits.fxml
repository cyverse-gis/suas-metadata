<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<?import javafx.geometry.Insets?>
<?import java.net.URL?>
<BorderPane fx:controller="controller.homeView.CreditsController" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml">
    <padding>
        <Insets top="10" bottom="10" right="10" left="10"/>
    </padding>
    <stylesheets>
        <URL value="@Credits.css"/>
    </stylesheets>
    <top>
        <Label BorderPane.alignment="CENTER" text="SANIMAL Credits">
            <font>
                <Font size="32"/>
            </font>
            <padding>
                <Insets top="5" bottom="5"/>
            </padding>
        </Label>
    </top>
    <center>
        <GridPane styleClass="creditsPane" BorderPane.alignment="CENTER" alignment="CENTER" vgap="10" hgap="20">
            <columnConstraints>
                <ColumnConstraints/>
                <ColumnConstraints/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints/>
            </rowConstraints>
            <Label GridPane.columnIndex="0" GridPane.rowIndex="0" text="Lead Developer:"/>
            <Label GridPane.columnIndex="1" GridPane.rowIndex="0" text="David Slovikosky"/>
        </GridPane>
    </center>
</BorderPane>
